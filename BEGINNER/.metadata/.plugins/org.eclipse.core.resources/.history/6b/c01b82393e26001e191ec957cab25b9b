package beginArrays;

public class Array{
	private int[] items;
	private int count=0;
	public Array(int length) {
		items = new int[length];
	}
	
	public void insert(int item) {
		if(count==items.length) {
			
		}
		items[count++]=item;
	}
	
	public void print() {
		for(int i=0; i<count;i++) {
			System.out.print(items[i]+ " ");
		}
		System.out.print("\n");
	}
}